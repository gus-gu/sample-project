  <modelVersion>4.0.0</modelVersion>
  <groupId>cn.com.datasolution</groupId>
  <artifactId>dkservice</artifactId>
  <version>1.0-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.3.0</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration>
              <filters>
                <filter>
                  <artifact>*:*</artifact>
                  <excludes>
                    <exclude>META-INF/*.SF</exclude>
                    <exclude>META-INF/*.DSA</exclude>
                    <exclude>META-INF/*.RSA</exclude>
                  </excludes>
                </filter>
              </filters>
              <transformers>
                <transformer>
                  <mainClass>cn.com.datasolution.dkservice.DKserviceApplication</mainClass>
                </transformer>
                <transformer>
                  <resource>META-INF/spring.factories</resource>
                </transformer>
              </transformers>
            </configuration>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-maven-plugin</artifactId>
        <version>2.4.5</version>
        <executions>
          <execution>
            <goals>
              <goal>repackage</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.19</version>
        <configuration>
          <additionalClasspathElements>
            <additionalClasspathElement>D:\greensoftware\pdi-ce-9.2.0.0-290\data-integration\lib</additionalClasspathElement>
            <additionalClasspathElement>D:\greensoftware\pdi-ce-9.2.0.0-290\data-integration\libswt\win64</additionalClasspathElement>
            <additionalClasspathElement>D:\greensoftware\pdi-ce-9.2.0.0-290\data-integration\classes</additionalClasspathElement>
          </additionalClasspathElements>
        </configuration>
      </plugin>
    </plugins>
  </build>
